<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?>

<plugin>

	<!-- Tabs -->
	
  <extension id="YASPPT" point="org.protege.editor.core.application.WorkspaceTab">
		<label value="YASPPTab"/>
		<class value="dmi.protege.YASPP.tab.YASPPTab"/>
		<index value="Y"/>
		<editorKitId value="OWLEditorKit"/>
		<defaultViewConfigFileName value="viewconfig-yasppt.xml"/>
   </extension>

	<!-- View -->

	<extension id="YASPPI" point="org.protege.editor.core.application.ViewComponent">
		<label value="YASPP Query" />
		<class value="dmi.protege.YASPP.view.YASPPInputView" />
		<headerColor value="@org.protege.ontologycolor"/>
		<category value="@org.protege.querycategory" />
	</extension>          
</plugin>
